POST          /api/login                 controllers.UserController.login()
POST          /api/logout                controllers.UserController.logout()
POST          /api/changepassword        controllers.UserController.changePassword()
GET           /api/user                  controllers.UserController.user()

GET           /api/source                controllers.WoofController.listSources()
POST          /api/source                controllers.WoofController.createSource()
DELETE        /api/source/:source        controllers.WoofController.deleteSource(source)

POST          /api/sync/:source          controllers.WoofController.syncSource(source, history: Int)
GET           /api/peek/:source          controllers.WoofController.peekWoof(source)
GET           /api/query                 controllers.WoofController.queryWoofs(source, from: Option[Long], to: Option[Long], interval: Interval, aggregation: Aggregation, raw_elements: Option[Int])

